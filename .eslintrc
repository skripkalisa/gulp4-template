{
  "env": {
    "commonjs": true,
    "es6": true,
    "node": true,
    "browser": true
  },

  "extends": [
    //"airbnb-base",
    "eslint:recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 2015,
    "sourceType": "module"
  },
  "rules": {
    "no-console": 0,
    // "indent": [
    //   2,
    //   2
    // ],
    "quotes": [
      2,
      "single"
    ],
    "semi": [
      2,
      "never"
    ]
  },
  "globals": {
    "document": false
  }
}